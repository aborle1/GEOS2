pgsbindir=`pwd`

if [ -f pgs-dev-env.csh.cpp ] ; then
# get $1 $2 $3 which are PGSHOME, HDFHOME and HDFEOS_HOME of pgs-dev-env.csh.cpp
set -- `awk 'NR == 1 , NR == 3 {print $3}' pgs-dev-env.csh.cpp`

    # if there exists a directory matched $1 which is PGSHOME
    if [ -d $1 ] ; then

	# if there exists a directory matched $1/bin/$dir which is PGSBIN of pgs-dev-env.csh.cpp
	if [ -d $1/bin/$dir ] ; then
	RETVAL= cd $1/bin/$dir
	RETVAL=`expr $?`

