	# contents_of_pgsbindir1 (if we just compare pgsbindir and pgsbindir1, we may have problems
	# with the link of directories)
	pgsbindir1=`pwd`
	contents_of_pgsbindir=`ls -al $pgsbindir`
	contents_of_pgsbindir1=`ls -al $pgsbindir1`

	    if [ "$contents_of_pgsbindir" = "$contents_of_pgsbindir1" ] ; then
